{
  "version": 2,
  "builds": [
    {
      "src" : "*.js",
      "use" :"@vercel/node"
    }
  ],
  "routes": [
   {
    "src" : "/(.*)",
    "dest" : "/"
   }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "cleanUrls": true
}
